{
	"name": "actyx-pond",
  "displayName": "Actyx Pond",
  "publisher": "Actyx",
	"description": "Create new fishes for your Actyx Pond in seconds, and define your commands and events from a high level syntax.",
  "version": "0.1.0",
	"engines": {
		"vscode": "^1.40.0"
  },
  "repository": {
    "url": "https://github.com/Actyx/vscode-actyx-pond"
  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:actyx.newFish",
		"onCommand:actyx.events",
		"onCommand:actyx.commands"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "actyx.newFish",
				"title": "Actyx: new fish"
			},
			{
				"command": "actyx.events",
				"title": "Actyx: create events"
			},
			{
				"command": "actyx.commands",
				"title": "Actyx: create commands"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"prettier": "^1.19.1",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^9.0.0",
		"tslint-immutable": "^6.0.1",
		"tslint-plugin-prettier": "^2.0.1",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
